{
  "name": "@fatso83/retry-dynamic-import",
  "private": false,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf types dist demo/dist",
    "dev": "jest --watch",
    "dev:demo": "vite -c demo/vite.config.ts --force demo/ ",
    "build:demo-gh-pages": "npm run build:demo -- --base=/retry-dynamic-import",
    "build:demo": "tsc && vite build -c demo/vite.config.ts demo",
    "build:types": "tsc --project ./tsconfig.build.json",
    "build:dist": "vite build",
    "build": "run-p build:*",
    "prepreview:demo": "npm run build:demo",
    "preview:demo": "vite preview -c demo/vite.config.ts demo/",
    "prepublishOnly": "npm run build",
    "postpublish": "./scripts/postpublish.sh",
    "postversion": "npm publish",
    "test": "jest"
  },
  "exports": {
    ".": "./dist/index.js",
    "./react-lazy": "./dist/react-lazy.js"
  },
  "files": [
    "dist/*.js",
    "types/*.d.ts"
  ],
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "^4.0.0",
    "debug": "^4.3.4",
    "esbuild": "0.17.18",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "5",
    "vite": "4"
  }
}
