{
  "name": "@fatso83/retry-dynamic-import",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist demo/dist",
    "dev:demo": "vite -c demo/vite.config.ts --force demo/ ",
    "build:demo": "tsc && vite build -c demo/vite.config.ts demo/",
    "build:dist": "tsc --project ./tsconfig.build.json --outDir dist",
    "build": "npm run clean && run-p build:*",
    "preview:demo": "vite preview -c demo/vite.config.ts demo/",
    "test": "jest"
  },
  "main": "./dist/index.js",
  "files": [
    "dist/*.js",
    "dist/*.d.ts"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "^4.0.0",
    "esbuild": "0.17.18",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "5",
    "vite": "4"
  },
  "optionalDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  }
}
